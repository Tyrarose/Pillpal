<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:id="@+id/frameStacktimeB"
        style="@style/groupStylegreen_200cornerRadius"
        android:layout_width="@dimen/_115pxh"
        android:layout_height="@dimen/_30pxv"
        android:layout_marginEnd="5dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent">

        <TextView
            android:id="@+id/txtTimeB"
            style="@style/txtPoppinsbold13_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:layout_marginStart="@dimen/_26pxh"
            android:layout_marginTop="@dimen/_5pxv"
            android:layout_marginEnd="@dimen/_27pxh"
            android:layout_marginBottom="@dimen/_4pxv"
            android:gravity="top|start"
            android:singleLine="true"
            android:text="09:00 AM"
            android:textColor="@color/White" />
    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
