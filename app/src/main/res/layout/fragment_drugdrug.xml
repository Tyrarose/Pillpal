<?xml version="1.0" encoding="UTF-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_interaction_result"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <FrameLayout android:id="@+id/frameDruginteractio"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:background="@color/Bluelight">

        <LinearLayout
            android:id="@+id/linearColumnlanguage"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="top|start|center"
            android:layout_marginStart="28.89dp"
            android:layout_marginTop="10.43dp"
            android:layout_marginEnd="22.89dp"
            android:layout_marginBottom="20dp"
            android:orientation="vertical">

            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent">
                <FrameLayout android:id="@+id/__link_drug_interaction_empty__pillpal_logo"
                    android:layout_height="30dp"
                    android:layout_width="56dp"
                    android:background="@drawable/pillpal_logo">
                </FrameLayout>
            </androidx.appcompat.widget.Toolbar>

            <TextView
                android:id="@+id/txtPrice"
                android:textSize="12dp"
                android:fontFamily="@font/robotoromanbold"
                android:textColor="@color/Black"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginStart="22.89dp"
                android:layout_marginTop="36.03dp"
                android:layout_marginEnd="22.89dp"
                android:ellipsize="end"
                android:lineSpacingExtra="3.43dp"
                android:singleLine="true"
                android:text="@string/msg_add_drug_to_che" />
            <androidx.appcompat.widget.SearchView
                android:id="@+id/searchView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:defaultQueryHint="Tylenol"
                app:iconifiedByDefault="false"
                android:layout_margin="8dp"/>
            <ListView
                android:visibility="gone"
                android:id="@+id/listView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/searchView"
                android:layout_marginTop="8dp"/>

            <LinearLayout
                android:id="@+id/buttonContainer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
            </LinearLayout>
































            <LinearLayout
                android:visibility="visible"
                android:id="@+id/threeButtonsGroup"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:layout_gravity="center">
            <TextView
                android:id="@+id/txtAddtwodrugs"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:layout_marginStart="21.74dp"
                android:layout_marginTop="13.27dp"
                android:layout_marginEnd="21.74dp"
                android:ellipsize="end"
                android:fontFamily="@font/inter"
                android:lineSpacingExtra="3.43dp"
                android:singleLine="true"
                android:textColor="@color/Black"
                android:textSize="12sp"
                android:text="@string/lbl_add_two_drugs" />



            <!--                style="@style/btnSolidRounded"-->
            <Button

                style="@style/btnSolidRounded"
                android:id="@+id/btnCheckInteractions"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="16.12dp"
                android:layout_marginRight="20dp"
                android:ellipsize="end"
                android:gravity="center"
                android:lineSpacingExtra="6.87dp"
                android:paddingStart="34.33dp"
                android:paddingTop="11.38dp"
                android:paddingEnd="34.33dp"
                android:paddingBottom="11.38dp"
                android:singleLine="true"
                android:text="@string/msg_check_interacti"
                android:textAllCaps="false"/>

            <LinearLayout
                android:id="@+id/linearRowclear"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="9.48dp"
                android:layout_marginRight="20dp"
                android:orientation="horizontal">


                <Button
                    style="@style/btnSolidRounded_1"
                    android:id="@+id/btnClear"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="10dp"
                    android:layout_weight="0.5"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:lineSpacingExtra="6.87dp"
                    android:paddingStart="34.33dp"
                    android:paddingTop="11.38dp"
                    android:paddingEnd="34.33dp"
                    android:paddingBottom="11.38dp"
                    android:singleLine="true"
                    android:text="@string/lbl_clear"
                    android:textAllCaps="false"/>


<!--                style="@style/btnSolidRoundedOutline"-->
                <Button
                    style="@style/btnSolidRoundedOutline"
                    android:id="@+id/btnLoadExample"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="1.14dp"
                    android:layout_weight="0.5"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:lineSpacingExtra="6.87dp"
                    android:singleLine="true"
                    android:textAllCaps="false"
                    android:text="@string/lbl_load_example" />
            </LinearLayout>

            </LinearLayout>


            <TextView
                android:id="@+id/txtInteractionResult"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:text=""
                android:textSize="18sp"
                android:textColor="@android:color/black"
                android:layout_centerHorizontal="true"/>






            <LinearLayout
                android:id="@+id/drugdrugResults"
                android:visibility="gone"
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:orientation="vertical">
                    <LinearLayout
                        android:layout_gravity="center"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <LinearLayout
                            android:gravity="center"
                            style="@style/groupStylered_300cornerRadius"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_70pxv"
                            android:layout_width="wrap_content"
                            android:layout_marginBottom="10dp">
                            <TextView
                                android:background="@drawable/rectangle_bg_white_a700_border_bluegray_800_19_radius_tl_15_bl_15"
                                android:id="@+id/drugOne"
                                android:layout_margin="2dp"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                style="@style/txtPoppinsbold15_1v2"
                                android:paddingRight="20dp"
                                android:paddingLeft="20dp"
                                android:layout_width="wrap_content"
                                android:lineSpacingExtra="@dimen/_2pxh"
                                android:text="@string/lbl_warfarin" />
                            <ImageView
                                android:id="@+id/compareImage"
                                android:layout_width="wrap_content"
                                android:layout_height="@dimen/_19pxh"
                                android:layout_gravity="center"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_minimize"
                                tools:ignore="ContentDescription"
                                tools:src="@drawable/img_minimize" />

                            <TextView
                                android:id="@+id/drugTwo"
                                android:layout_width="wrap_content"
                                android:paddingRight="20dp"
                                android:paddingLeft="20dp"
                                android:layout_height="match_parent"
                                android:layout_margin="2dp"
                                style="@style/txtPoppinsbold15_1v2"
                                android:background="@drawable/rectangle_bg_white_a700_border_bluegray_800_19_radius_tr_15_br_15"
                                android:gravity="center"
                                android:lineSpacingExtra="@dimen/_2pxh"
                                android:text="@string/msg_acetaminophen" />
                        </LinearLayout>
                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="5dp"
                            android:layout_gravity="center">
                            <TextView
                                android:id="@+id/txtSEVERITYOne"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="start"
                                android:ellipsize="end"
                                android:gravity="top|start"
                                android:lineSpacingExtra="@dimen/_3pxh"
                                android:singleLine="true"
                                style="@style/txtRobotoromanbold15_1"
                                android:text="@string/lbl_severity" />
                            <ImageView
                                android:id="@+id/imageQuestion"
                                android:layout_width="@dimen/_14pxh"
                                android:layout_height="@dimen/_14pxh"
                                android:layout_marginStart="@dimen/_2pxh"
                                android:layout_marginTop="@dimen/_2pxv"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_question_white_a700" />
                        </LinearLayout>
                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end"
                            android:id="@+id/btnModerateOne"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_30pxv"
                            android:layout_width="@dimen/_115pxh"
                            android:lineSpacingExtra="@dimen/_6pxh"
                            android:paddingBottom="@dimen/_6pxv"
                            android:paddingEnd="@dimen/_24pxh"
                            android:paddingStart="@dimen/_25pxh"
                            android:paddingTop="@dimen/_6pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_moderate"
                            android:textAllCaps="false"
                            android:layout_marginBottom="20dp"
                            style="@style/btnSolidRounded_5" tools:text="@string/lbl_moderate"/>
                    </LinearLayout>

                <ScrollView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:overScrollMode="always"
                    android:scrollbars="vertical">

                    <LinearLayout
                        android:id="@+id/linearColumnpriceSeven"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/txtPriceSeven"
                            style="@style/txtRobotoromanbold15_1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginStart="@dimen/_55pxh"
                            android:layout_marginEnd="@dimen/_55pxh"
                            android:ellipsize="end"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@string/lbl_description" />

                        <TextView
                            android:id="@+id/txtDescriptionSix"
                            style="@style/txtInterregular12"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_marginEnd="@dimen/_1pxh"
                            android:gravity="center_horizontal"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:text="@string/msg_lorem_ipsum_dol" />
                        <TextView
                            android:id="@+id/txtPriceEight"
                            style="@style/txtRobotoromanbold15_1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_55pxh"
                            android:layout_marginTop="@dimen/_23pxv"
                            android:layout_marginEnd="@dimen/_52pxh"
                            android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@string/msg_extended_descri" />

                        <TextView
                            android:id="@+id/txtDescriptionSeven"
                            style="@style/txtInterregular12"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginStart="@dimen/_1pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:gravity="center_horizontal"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:text="@string/msg_lorem_ipsum_dol2" />
                    </LinearLayout>
                </ScrollView>
            </LinearLayout>



            <LinearLayout android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
            <LinearLayout
                android:id="@+id/linearWarning"
                style="@style/groupStylecornerRadius_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_marginStart="22.89dp"
                android:layout_marginEnd="22.89dp"
                android:gravity="end"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/imageWarning"
                    android:layout_width="21.74dp"
                    android:layout_height="19.46dp"
                    android:layout_marginStart="12.59dp"
                    android:layout_marginTop="20.86dp"
                    android:layout_marginBottom="76.8dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_warning"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_warning" />
                <TextView
                    android:id="@+id/txtDescription"
                    android:textSize="12sp"
                    android:fontFamily="@font/inter"
                    android:textColor="@color/Black"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_9pxh"
                    android:layout_marginTop="@dimen/_18pxv"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginBottom="@dimen/_24pxv"
                    android:layout_weight="0.92"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:text="@string/msg_warning_if_no" />
            </LinearLayout>

            <ImageView
                android:id="@+id/imageBbcapcule"
                android:layout_width="@dimen/_29pxh"
                android:layout_height="@dimen/_80pxh"
                android:layout_gravity="center"
                android:layout_marginStart="@dimen/_126pxh"
                android:layout_marginTop="30dp"
                android:layout_marginEnd="@dimen/_126pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_bbcapcule_white_a700"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_bbcapcule_white_a700" />
            </LinearLayout>



































        </LinearLayout>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
